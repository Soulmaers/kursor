    <div class="wrapperFull">

        <div class="wrapper_right_dash">

            <div class="dash_card">
                <h2 class="dash_title">
                    Соотношение отклонений по давлению
                </h2>
                <div class="dash_board">
                    <canvas id="myChart"></canvas>
                </div>
            </div>
            <div class="configChange">
                <p class="title_change">Чекбокс</p>

                <form class="check_box">
                    <p class="p"><input class="input" type="checkbox" rel="" value="Все" id="Все" checked>Все</p>
                </form>
            </div>

        </div>



        <section class="section">
            <div class="list_att">
                <div class="list_item1">Машина</div>
                <div class="list_item2">Тягач</div>
