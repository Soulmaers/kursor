                    <!-- Get the leaflet JavaScript file -->

                </div>
                <div class="speedGraf">
                    <div class="form_speed">
                        <label>Интервал
                            <select class="select_speed">
                                <option disabled selected value="0">Указанный интервал</option>
                                <option class="option_int" value="1">За сутки</option>
                                <option class="option_int" value="2">За неделю</option>
                                <option class="option_int" value="3">За месяц</option>
                            </select>
                        </label>
                        <label>От
